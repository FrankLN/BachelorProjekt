<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    @*<meta name="viewport" content="width=device-width, initial-scale=1.0">*@
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />

    <title>@ViewBag.Title - WebApp</title>

    @*Stylesheets*@
    <link href="http://code.jquery.com/ui/1.10.4/themes/smoothness/jquery-ui.min.css" rel="stylesheet" type="text/css" />

    <link href="~/Content/style/tableStyle.css?v=2" rel="stylesheet" type="text/css" />
    <link href="~/Content/style/homeStyle.css?v=2" rel="stylesheet" type="text/css" />
    <link href="~/Content/style/buttonStyle.css?v=2" rel="stylesheet" type="text/css" />
    <link href="~/Content/style/dropdownStyle.css?v=2" rel="stylesheet" type="text/css" />

    @*Javascripts*@
    <script src="http://code.jquery.com/jquery-1.10.2.js"></script>
    <script src="http://code.jquery.com/ui/1.11.4/jquery-ui.js"></script>

    <script src="~/Content/script/popupPatientScript.js?v=2"></script> 
    <script src="~/Content/script/datePickerScript.js?v=3"></script>
    <script src="~/Content/script/dropdownScript.js?v=1"></script>
</head>
<body>
    <wrapper>
        <header>
            <img src="~/Content/img/app_name.jpg" alt="Idun.net" style="width: 330px; height: 67px;"/>
            <h2> Login Info</h2>
        </header>

        <div class="container body-content">
            @RenderBody()
        </div>



    <div id="footer">
        <p class="terms"> Terms and conditions</p>
        <p class="copyright">&copy; @DateTime.Now.Year - WebApp</p>
    </div>
    </wrapper>
</body>
</html>