    <script src="http://code.jquery.com/ui/1.10.4/jquery-ui.min.js"></script>
    <script src="~/Content/script/datePicker.js?v=3"></script>
    <script src="~/Content/script/dropdownScript.js?v=1"></script>
</head>
<body>
    <wrapper>
        <header>
            <img src="~/Content/img/app_name.jpg" alt="Idun.net" style="width: 330px; height: 67px;"/>
            <h2> Login Info</h2>
        </header>
